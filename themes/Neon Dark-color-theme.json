	"editor.tokenColorCustomizations": {
		"numbers": "#f6ec28",
		"variables": "#8edbab",
		"functions": "#1499ad",
		"keywords": "#831bc4",
		"types": "#ff4444",
		"strings": "#9c8d00",
		"textMateRules": [
			{
				"scope": [
					"keyword.type",
					"keyword.other.namespace",
					"keyword.other.using.",
					"keyword.other.class",
					"keyword.other.new",
					"storage.type.function.arrow",
					"entity.name.type"
				],
				"settings": {
					"foreground": "#0365a2"
				}
			},
			{
				"scope": [
					"punctuation.terminator.statement",
					"punctuation.separator.comma",
					"punctuation.accessor"
				],
				"settings": {
					"foreground": "#bd0373"
				}
			},
			{
				"scope": [
					"keyword.operator.assignment",
					"keyword.operator.arithmetic",
					"keyword.operator.comparison"
				],
				"settings": {
					"foreground": "#c0015a"
				}
			},

			{
				"scope": [
					"punctuation.parenthesis",
					"keyword.operator.logical",
					"storage.type"
				],
				"settings": {
					"foreground": "#f62451"
				}
			},

			{
				"scope": ["punctuation.squarebracket"],
				"settings": {
					"foreground": "#6323ed"
				}
			},

			{
				"scope": ["punctuation.curlybrace"],
				"settings": {
					"foreground": "#defa29"
				}
			},
			{
				"scope": ["entity.other.attribute-name"],
				"settings": {
					"foreground": "#ff6e5b"
				}
			}
		]
	}
}
